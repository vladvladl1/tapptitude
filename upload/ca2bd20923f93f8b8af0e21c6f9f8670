<script>
	let x=2, y=3, z = myfunc(x, y+1);
function myfunc(p, l){
	return p*l;
}
document.getElementById("demo").innerHTML = z;

const car = {name:"audi", model:"a3", year:1996};

car.name = "suzuki";

document.getElementById("demo1").innerHTML = car.name;

const om = {nume:"cineva", prenume:"altcineva", 
  fullName : function(){
  	return this.nume + " " + this.prenume;
  }
};
document.getElementById("demo2").innerHTML = om.fullName();

function displayDate(){
	document.getElementById("demo3").innerHTML =  Date();
}

let text = "acesti \"copii\" nemernici";
document.getElementById("demo5").innerHTML = text;

let doi = "ceva, altceva, nimic";
document.getElementById("demo6").innerHTML = doi.slice(5, 10);
const doisplited = doi.split(",");
document.getElementById("demo7").innerHTML = doisplited[1];
let trei = "salutare amice";
document.getElementById("demo8").innerHTML = doi.includes("al");

//cred ca nici safari nu suporta asta
let fn = "andrei";
let ln = "minciuna";
let txt = 'salut ${fn} ${ln}';
document.getElementById('demo9').innerHTML = txt;
//dar e ok

let mynum  = 52;
let newnu = new Number(19);
document.getElementById("demo10").innerHTML = typeof mynum.toString(2);
document.getElementById('demo11').innerHTML =  typeof newnu;
let r = 9.656;
document.getElementById("demo12").innerHTML = 
  r.toPrecision() + "<br>" +
  r.toPrecision(1) + "<br>" +
  r.toPrecision(4) + "<br>" +
  r.toExponential(6); 

document.getElementById("demo13").innerHTML = Number("12");

const cars = ["bmw", "audi", "mitsubishi"];


